,sBAJF,EAIE;AAHA,IAAA,GAAG,EAAC,SAGJ;AAFC,oBAAc,IAAA,CAEf;AADC,IAAA,GAAG,EAAE,KAAA,CAAA;AACN,GAJF,E,IAAA,E;;AAAA,I,OAAA,CAKI,CANN,CAmIQ,EA5HR,mBAAA,CA0HM,KA1HN,EAAA,UAAA,EA0HM,CAzHJ,mBAAA,CAAA,4nDAAA,CAyHI,EAjDJ,mBAAA,CAMI,GANJ,EAMI;AALF,IAAA,IAAI,EAAC,GAKH;AAJF,IAAA,IAAI,EAAC,QAIH;AAHD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,SAAA,IAAA,QAAA,CAAA,SAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAmB,CAAA,SAAA,CAAnB,CAAA;AAGJ,GANJ,EAIC,QAJD,CAiDI,EA1Cd,mBAAA,CAAA,izBAAA,CA0Cc,EAPJ,mBAAA,CAMI,GANJ,EAMI;AALF,IAAA,IAAI,EAAC,GAKH;AAJF,IAAA,IAAI,EAAC,QAIH;AAHD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,eAAA,IAAA,QAAA,CAAA,eAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAyB,CAAA,SAAA,CAAzB,CAAA;AAGJ,GANJ,EAIC,gBAJD,CAOI,CA1HN,CA4HQ,CApIV,CAiJI,EAZJ,mBAAA,CAWU,SAXV,EAAA,UAAA,EAWU,CATR,UASQ,EARR,mBAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CALI,KAAA,CAAA,O,kBADR,mBAAA,CAIE,KAJF,EAIE;UAAA;AAFC,IAAA,GAAG,EAAE,KAAA,CAAA,OAEN;AADA,IAAA,GAAG,EAAC;AACJ,GAJF,E,IAAA,E;;AAAA,I,UAAA,C,mBAKA,mBAAA,CAAuC,KAAvC,EAAA,UAAA,C,CACI,CAPN,CAQQ,CAXV,CAYI,CAlJN,CAmJI,CA7JN,C","sourcesContent":["<template>\n  <div>\n    \n    <input\n      ref=\"input\"\n      type=\"file\"\n      name=\"image\"\n      accept=\"image/*\"\n      @change=\"setImage\"\n    />\n\n    <div class=\"content\">\n      <section class=\"cropper-area\">\n        <div class=\"img-cropper\">\n          <vue-cropper\n            ref=\"cropper\"\n            :aspect-ratio=\"8 / 8\"\n            :src=\"imgSrc\"\n          />\n        </div>\n        <div class=\"actions\">\n          <!-- <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"zoom(0.2)\"\n          >\n            Zoom In\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"zoom(-0.2)\"\n          >\n            Zoom Out\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"move(-10, 0)\"\n          >\n            Move Left\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"move(10, 0)\"\n          >\n            Move Right\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"move(0, -10)\"\n          >\n            Move Up\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"move(0, 10)\"\n          >\n            Move Down\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"rotate(90)\"\n          >\n            Rotate +90deg\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"rotate(-90)\"\n          >\n            Rotate -90deg\n          </a>\n          <a\n            ref=\"flipX\"\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"flipX\"\n          >\n            Flip X\n          </a>\n          <a\n            ref=\"flipY\"\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"flipY\"\n          >\n            Flip Y\n          </a> -->\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"cropImage\"\n          >\n            Crop\n          </a>\n<!--           <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"reset\"\n          >\n            Reset\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"getData\"\n          >\n            Get Data\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"setData\"\n          >\n            Set Data\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"getCropBoxData\"\n          >\n            Get CropBox Data\n          </a>\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"setCropBoxData\"\n          >\n            Set CropBox Data\n          </a> -->\n          <a\n            href=\"#\"\n            role=\"button\"\n            @click.prevent=\"showFileChooser\"\n          >\n            Upload Image\n          </a>\n        </div>\n\n      </section>\n      <section class=\"preview-area\">\n       \n        <p>Cropped Image</p>\n        <div class=\"cropped-image\">\n          <img\n            v-if=\"cropImg\"\n            :src=\"cropImg\"\n            alt=\"Cropped Image\"\n          />\n          <div v-else class=\"crop-placeholder\" />\n        </div>\n      </section>\n    </div>\n  </div>\n</template>\n\n<script>\nimport VueCropper from 'vue-cropperjs';\nimport 'cropperjs/dist/cropper.css';\nimport board from '../assets/forcrop.png';\n\nexport default {\n  name : \"demoCrop\",\n  components: {\n    VueCropper,\n  },\n  data() {\n    return {\n      imgSrc: board,\n      cropImg: '',\n      data: null,\n    };\n  },\n  methods: {\n    cropImage() {\n      // get image data for post processing, e.g. upload or setting image src\n      this.cropImg = this.$refs.cropper.getCroppedCanvas().toDataURL();\n    },\n    flipX() {\n      const dom = this.$refs.flipX;\n      let scale = dom.getAttribute('data-scale');\n      scale = scale ? -scale : -1;\n      this.$refs.cropper.scaleX(scale);\n      dom.setAttribute('data-scale', scale);\n    },\n    flipY() {\n      const dom = this.$refs.flipY;\n      let scale = dom.getAttribute('data-scale');\n      scale = scale ? -scale : -1;\n      this.$refs.cropper.scaleY(scale);\n      dom.setAttribute('data-scale', scale);\n    },\n    getCropBoxData() {\n      this.data = JSON.stringify(this.$refs.cropper.getCropBoxData(), null, 4);\n    },\n    getData() {\n      this.data = JSON.stringify(this.$refs.cropper.getData(), null, 4);\n    },\n    move(offsetX, offsetY) {\n      this.$refs.cropper.move(offsetX, offsetY);\n    },\n    reset() {\n      this.$refs.cropper.reset();\n    },\n    rotate(deg) {\n      this.$refs.cropper.rotate(deg);\n    },\n    setCropBoxData() {\n      if (!this.data) return;\n\n      this.$refs.cropper.setCropBoxData(JSON.parse(this.data));\n    },\n    setData() {\n      if (!this.data) return;\n\n      this.$refs.cropper.setData(JSON.parse(this.data));\n    },\n    setImage(e) {\n      const file = e.target.files[0];\n\n      if (file.type.indexOf('image/') === -1) {\n        alert('Please select an image file');\n        return;\n      }\n\n      if (typeof FileReader === 'function') {\n        const reader = new FileReader();\n\n        reader.onload = (event) => {\n          this.imgSrc = event.target.result;\n          // rebuild cropperjs with the updated source\n          this.$refs.cropper.replace(event.target.result);\n        };\n\n        reader.readAsDataURL(file);\n      } else {\n        alert('Sorry, FileReader API not supported');\n      }\n    },\n    showFileChooser() {\n      this.$refs.input.click();\n    },\n    zoom(percent) {\n      this.$refs.cropper.relativeZoom(percent);\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style>\nbody {\n  font-family: Arial, Helvetica, sans-serif;\n  width: 1024px;\n  margin: 0 auto;\n}\n\ninput[type=\"file\"] {\n  display: none;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 10px 0 5px 0;\n}\n\n.header h2 {\n  margin: 0;\n}\n\n.header a {\n  text-decoration: none;\n  color: black;\n}\n\n.content {\n  display: flex;\n  justify-content: space-between;\n}\n\n.cropper-area {\n  width: 1024px;\n}\n\n.actions {\n  margin-top: 1rem;\n}\n\n.actions a {\n  display: inline-block;\n  padding: 5px 15px;\n  background: #0062CC;\n  color: white;\n  text-decoration: none;\n  border-radius: 3px;\n  margin-right: 1rem;\n  margin-bottom: 1rem;\n}\n\n\n\n.preview-area {\n  width: 307px;\n}\n\n.preview-area p {\n  font-size: 1.25rem;\n  margin: 0;\n  margin-bottom: 1rem;\n}\n\n.preview-area p:last-of-type {\n  margin-top: 1rem;\n}\n\n.preview {\n  width: 100%;\n  height: calc(372px * (9 / 16));\n  overflow: hidden;\n}\n\n.crop-placeholder {\n  width: 100%;\n  height: 640px;\n  background: #ccc;\n}\n\n.cropped-image img {\n  max-width: 100%;\n}\n</style>"],"sourceRoot":""}]}